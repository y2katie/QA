{
  "rewrites": [
    {
      "source": "/",
      "destination": "/home/index.html"
    },
    {
      "source": "/home",
      "destination": "/home/index.html"
    },
    {
      "source": "/day-1",
      "destination": "/day%201/index.html"
    },
    {
      "source": "/day-2", 
      "destination": "/day%202/index.html"
    },
    {
      "source": "/day-3",
      "destination": "/day%203/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
